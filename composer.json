{
    "name": "ozankurt/tracker",
    "description": "A Laravel Visitor Tracker",
    "keywords": ["tracker", "tracking", "visitor", "logging", "user agent", "mobile detection", "laravel", "kurt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ozan Kurt",
            "email": "me@ozankurt.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "~10|~11",
        "ozankurt/agent": "^1.0",
        "ua-parser/uap-php" : "^3.9",
        "snowplow/referer-parser": "^0.2",
        "geoip2/geoip2": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "OzanKurt\\Tracker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
               "OzanKurt\\Tracker\\TrackerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
